[alias]
	st = status
	br = branch
	co = checkout
	ci = commit
	df = diff
	hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
	last = cat-file commit HEAD
[core]
	editor = vim
    whitespace = -trailing-space,-indent-with-non-tab,-tab-in-indent
    quotepath = false

[merge]
	tool = kdiff3

[mergetool "kdiff3"]
	path = /usr/bin/kdiff3
[diff]
	tool=vimdiff
[difftool]
        prompt = false

[oh-my-zsh]
    hide-dirty = 1

[credential]
    helper = store

